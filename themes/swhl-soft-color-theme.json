		"terminal.foreground": "#CCCCCC",
		"terminal.ansiBlack": "#333333",
		"terminal.ansiBrightBlack": "#959595",
		"terminal.ansiRed": "#FDA293",
		"terminal.ansiBrightRed": "#FDA293",
		"terminal.ansiGreen": "#BEDD9F",
