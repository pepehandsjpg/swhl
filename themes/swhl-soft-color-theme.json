		"editorInlayHint.parameterForeground": "#959595",
		"editorInlayHint.typeForeground": "#959595"
	},
	// It's only Go specific semantic tokens, might extend it in future
	// highlighted groups: strings, numbers, constants, keywords, types, comments
	// https://cs.opensource.google/go/x/tools/+/master:gopls/doc/semantictokens.md
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"comment": {
			"foreground": "#FDA293",
			"fontStyle": ""
		},
		"keyword": {
			"foreground": "#AED7F5",
			"fontStyle": ""
		},
		"type": {
			"foreground": "#D6D4A6",
			"fontStyle": ""
		},
		"type.defaultLibrary": {
			"foreground": "#D6D4A6",
			"fontStyle": ""
		},
		"string": {
			"foreground": "#BEDD9F",
			"fontStyle": ""
		},
		"number": {
			"foreground": "#E6A4DF",
			"fontStyle": ""
		},
		"*.readonly": {
			"foreground": "#F4C1CF",
			"fontStyle": ""
		},
		"*.readonly.defaultLibrary": {
			"foreground": "#F4C1CF",
			"fontStyle": ""
		},
		"typeParameter": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"parameter": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"variable": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"method": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"function": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"operator": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		},
		"*.definition": {
			"foreground": "#CCCCCC",
			"fontStyle": ""
		}
	},
	"tokenColors": [
		{
			"scope": "string",
			"settings": {
				"foreground": "#BEDD9F"
			}
		},
		{
			"scope": "source.yaml",
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": ""
			}
		},
		{
			"scope": "source.makefile",
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": ""
			}
		}
	]
}